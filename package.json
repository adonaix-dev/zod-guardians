{
    "name": "zod-guardians",
    "version": "0.1.1",
    "type": "module",
    "description": "Type-safe overloads and argument validation with Zod.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adonaix-dev/zod-guardians.git"
    },
    "author": "Arthur Adonai <>",
    "license": "MIT",
    "packageManager": "bun@1.3.4",
    "engines": {
        "node": "^20.0.0"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "zod",
        "typescript",
        "validation",
        "arguments",
        "parameters",
        "function",
        "overload",
        "overloading",
        "schema",
        "type-safe",
        "runtime-validation",
        "signatures",
        "rest-parameters",
        "variadic",
        "type-guard"
    ],
    "scripts": {
        "build": "rollup -c",
        "prepublishOnly": "bun run build"
    },
    "dependencies": {
        "type-fest": "^5.3.1",
        "zod": "^4.2.0",
        "zod-validation-error": "^5.0.0"
    },
    "devDependencies": {
        "@adonaix/types": "link:@adonaix/types",
        "@changesets/cli": "^2.29.8",
        "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/bun": "1.3.4",
        "acorn": "^8.15.0",
        "adonaix-lib": "link:adonaix-lib",
        "astring": "^1.9.0",
        "prettier": "^3.7.4",
        "prettier-plugin-jsdoc": "^1.8.0",
        "prettier-plugin-pkg": "^0.21.2",
        "rollup": "^4.53.3",
        "rollup-plugin-dts": "^6.3.0",
        "rollup-plugin-prettier": "^4.1.2",
        "source-map-js": "^1.2.1",
        "tslib": "^2.8.1"
    }
}
